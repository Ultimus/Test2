package TowerAlpha;

public class Rumkanone extends Tower {

public Rumkanone(int x, int y){
		
		super(x,y);
		this.damage = 2;
		this.level = 1;
		this.range = 180;
		this.size = 60;
		this.speed = 10;
		this.coolDown = 90;
		this.waited = 0; //Turm hat 0 Sekunden zu Beginn gewartet bis er zuletzt geschossen hat
		this.cost = 180;
		this.effect = 1; //tower slowed
		this.type = 2;
		this.upgradeCost = 500;
	}

		
}
